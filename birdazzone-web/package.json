    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "happy-dom": "^7.6.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
